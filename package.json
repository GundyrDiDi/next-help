{
  "name": "client-help",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env SERVER_ENV=pre next dev -p 4000",
    "test": "cross-env SERVER_ENV=test next start -p 3000",
    "jenkins_test": "cross-env SERVER_ENV=test next start -p 3000",
    "jenkins_pre": "cross-env SERVER_ENV=pre next dev -p 3000",
    "pre": "cross-env SERVER_ENV=pre next dev -p 4000",
    "api": "npx @snifftest/api-gen config hu  && npx eslint ./src/service --fix",
    "kill-test": "kill $(lsof -i :3000 | awk 'NR==2 {print $2}')",
    "build": "next build",
    "build:test": "cross-env SERVER_ENV=test next build",
    "testEcho": "echo 999 > x.txt",
    "build:pre": "cross-env SERVER_ENV=pre NEXT_PUBLIC_ENV=pre next build",
    "build:prod": "cross-env SERVER_ENV=prod next build",
    "start": "next start -p 3000",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint"
  },
